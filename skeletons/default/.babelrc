{
    "presets": ["react", "es2015", "stage-0"],
    "plugins": [
        ["module-resolver", {
            "alias": {
                "inventor": "./server/inventor",
                "@server": "./server",
                "@shared": "./shared",
                "@vendor": "./vendor",
                "@controllers": "./server/app/Http/Controllers",
                "@models": "./server/app/Models",
            }
        }],
	   "transform-decorators-legacy"
    ]
}
